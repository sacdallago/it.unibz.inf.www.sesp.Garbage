// Function used:

public static boolean stringIsSentence(String s){
		String literal = s.replaceAll("[^\\p{L}\\p{Nd}]", "");
		String numbers = literal.replaceAll("[^0-9]+", "");
		String punctuation = s.replaceAll("[^\\.|,|;|:|!|?|¿|¡|'|\"]+", "");
		
		System.out.println(s);
		System.out.println(literal);
		System.out.println(numbers);
		System.out.println(punctuation);
		System.out.println("\nTotal "+s.length()+" in % ="+(100*s.length())/s.length());
		System.out.println("Literals "+literal.length()+" in % ="+(100*literal.length())/s.length());
		System.out.println("Numbers "+numbers.length()+" in % ="+(100*numbers.length())/s.length());
		System.out.println("punctuation "+punctuation.length()+" in % ="+(100*punctuation.length())/s.length());
		
		return true;
	}
	
	
	
	
// Outcome:
Literals in % = 80/84
Numbers in % = 0/1
punctuation in % = 1/3
